class Xpattern {

	public static void main(String args[]) 
	{
			int size=5;
				for(int i=1;i<=size;i++)
				{
					
					for(int j=1; j<=size;j++)
					{
						if(i==j || (j==(size+1)-i) )
							System.out.print("*");
						else
							System.out.print("_");
						
					}
					System.out.println();
				}
	}
}
